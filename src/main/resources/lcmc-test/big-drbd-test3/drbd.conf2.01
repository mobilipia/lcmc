## generated by drbd-gui

global {
		usage-count	yes;
}

common {
	startup {
		degr-wfc-timeout	0;
	}

	net {
		cram-hmac-alg	sha1;
		shared-secret	SECRET;
	}

	disk {
		on-io-error	detach;
	}

}

resource r0 {
	on host1 {
		volume 0 {
			device		/dev/drbd0;
			disk		DISK;
			flexible-meta-disk	internal;
		}
		address		IP:7788;
	}
	on host2 {
		volume 0 {
			device		/dev/drbd0;
			disk		DISK;
			flexible-meta-disk	internal;
		}
		address		IP:7788;
	}
}
